<!doctype html>
<html>
	<head>
		<meta charset="UTF-8" />
		<title>prot√≥tipo</title>
		<script>
			function teste(){
var xmlhttp;
if (window.XMLHttpRequest)
  {// code for IE7+, Firefox, Chrome, Opera, Safari
  xmlhttp=new XMLHttpRequest();
  }
else
  {// code for IE6, IE5
  xmlhttp=new ActiveXObject("Microsoft.XMLHTTP");
  }
xmlhttp.onreadystatechange=function()
  {
  if (xmlhttp.readyState==4 && xmlhttp.status==200)
    {
    document.getElementById("resultado").innerHTML=xmlhttp.responseText;
    }
  }
xmlhttp.open("GET","/api/pacientes",true);
xmlhttp.send();
}
		</script>
	</head>
	<body>
		<div>
			<h3>Menu</h3>
			<a href="/pesquisa.html">pesquisar</a>
			<a href="/inclusao.html">incluir</a>
		</div>
		<div>
			<h1>SISTEMA DE CADASTRO DE PACIENTES</h1>
			<%= nome %><br />
			<input type="button" onclick="teste()" value="ajax" />
		</div>
		<div id="resultado">
		resultado
		</div>
	</body>
</html>